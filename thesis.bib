keywords = {ABO Blood-Group System,ABO Blood-Group System: genetics,Alleles,Genotype,Inuits,Inuits: genetics,Markov Chains,Pedigree,Phenotype},
month = jan,
number = {1},
pages = {1--18},
pmid = {1500799},
title = {{Sampling genotypes on complex pedigrees with phenotypic constraints: the origin of the B allele among the Polar Eskimos.}},
