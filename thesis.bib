issn = {1553-7404},
journal = {PLoS genetics},
month = apr,
number = {4},
pages = {e1002051},
pmid = {21552331},
title = {{Beyond missing heritability: prediction of complex traits.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3084207\&tool=pmcentrez\&rendertype=abstract},
volume = {7},
year = {2011}
}
