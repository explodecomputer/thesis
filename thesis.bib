journal = {American journal of human genetics},
keywords = {African Americans,African Americans: genetics,Asian Americans,Asian Americans: genetics,Asthma,Asthma: genetics,Catalase,Catalase: genetics,Child,Computer Simulation,Epistasis, Genetic,Genetic Predisposition to Disease,Genetic Testing,Genetic Testing: methods,Granulocyte Colony Stimulating Factor, Recombinant,Humans,Interleukin-3,Interleukin-3: genetics,Likelihood Functions,Models, Genetic,NAD(P)H Dehydrogenase (Quinone),NAD(P)H Dehydrogenase (Quinone): genetics,Recombinant Fusion Proteins,Recombinant Fusion Proteins: genetics},
month = jan,
